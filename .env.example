# Matrix Synapse environment variables
# Copy this to .env and fill in your values

# Database password for Synapse's PostgreSQL
SYNAPSE_DB_PASSWORD=your-secure-database-password

# Generate these with: python3 -c "import secrets; print(secrets.token_hex(32))"
SYNAPSE_MACAROON_SECRET=generate-a-random-secret
SYNAPSE_FORM_SECRET=generate-a-random-secret
SYNAPSE_REGISTRATION_SECRET=generate-a-random-secret

# TURN server shared secret (must match coturn/turnserver.conf and synapse/homeserver.yaml)
TURN_SHARED_SECRET=generate-a-random-secret
